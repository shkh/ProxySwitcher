{
  "manifest_version": 3,
  "name": "ProxySwitcher",
  "version": "1.0",
  "description": "Extension for switching proxies.",
  "permissions": [
    "proxy",
    "storage"
  ],
  "action": {
    "default_popup": "html/popup.html",
    "default_icon": {
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "128": "images/icon128.png"
  },
  "background": {
    "service_worker": "scripts/background.js"
  }
}
